{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser" : "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType" : "module"
  },
  
  "rules": {
    "no-console" : 0,
    "no-unused-vars" : 0,
    "no-undef" : 0
  }
}